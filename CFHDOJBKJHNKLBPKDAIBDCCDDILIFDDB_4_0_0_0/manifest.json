      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    },
    {
      "js": [
        "polyfill.js",
        "ext/common.js",
        "ext/content.js",
        "premium.preload.js",
        "premium-onboarding.preload.js"
      ],
      "matches": [
        "https://accounts.adblockplus.org/premium",
