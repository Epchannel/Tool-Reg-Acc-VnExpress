  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "vendor/@eyeo/webext-ad-filtering-solution/content.js",
        "polyfill.js",
        "ext/common.js",
        "ext/content.js",
        "composer.preload.js",
